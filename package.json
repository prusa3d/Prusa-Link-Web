{
  "name": "prusa-connect-local",
  "version": "1.0.0",
  "description": "This is a printer connect webservice. It is compatible with another 3D printer services, so many applications which use this service.",
  "private": false,
  "scripts": {
    "api:start": "swagger-repo serve",
    "api:build": "swagger-repo build -o docs",
    "api:test": "swagger-repo validate",
    "ui:build": "npm run ui:clean && npx webpack --config ./webpack.config.js --mode production --env.apiKey=developer --env.printer=sl1 --progress",
    "ui:dev": "npm run ui:clean && npx webpack --config ./webpack.config.js --mode development",
    "ui:start": "npx webpack-dev-server --config ./webpack.config.js --env.prod=1 --mode development",
    "ui:start-mini": "npx webpack-dev-server --config ./webpack.config.js --env.prod=1 --mode development --env.printer=mini",
    "ui:build-mini": "webpack --config ./webpack.config.js --mode production --env.apiKey=developer --env.printer=mini --progress",
    "ui:clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prusa3d/Prusa-Connect-Local.git"
  },
  "author": "Bruno Carvalho <brunoeee@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/prusa3d/Prusa-Connect-Local/issues"
  },
  "homepage": "https://github.com/prusa3d/Prusa-Connect-Local#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "bulma": "^0.8.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "glob-all": "^3.2.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^4.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preact-jsx-chai": "^2.3.2",
    "preact-markup": "^1.6.0",
    "preact-render-to-string": "^5.1.3",
    "preact-router": "^3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "rimraf": "^3.0.0",
    "sass": "^1.24.2",
    "sass-loader": "^8.0.1",
    "svg-url-loader": "^3.0.3",
    "swagger-repo": "^2.0.0-rc.15",
    "terser-webpack-plugin": "^2.3.8",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "preact": "^10.2.1",
    "preact-i18n": "^2.0.0-preactx.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
