{
  "openapi": "3.0.1",
  "info": {
    "title": "Connect",
    "description": "This is a printer connect webservice. It is compatible with another 3D printer services, so many applications which use this service",
    "contact": {
      "email": "developer@prusa3d.cz"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "externalDocs": {
    "description": "Find out more about API",
    "url": "http://connect.prusa3d.cz"
  },
  "tags": [
    {
      "name": "octoprint",
      "description": "Octoprint compatible"
    }
  ],
  "paths": {
    "/api/connection": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieve a mock for current connection settings.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "current": {
                      "$ref": "#/components/schemas/OctoConnCurrent"
                    },
                    "options": {
                      "$ref": "#/components/schemas/OctoConnOptions"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      },
      "post": {
        "tags": [
          "octoprint"
        ],
        "summary": "Mock for issue a connection command. Do not do anything.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/timelapse": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "summary": "Retrieves a moked list of timelapses and the current config.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "config": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string"
                        }
                      }
                    },
                    "enabled": {
                      "type": "boolean",
                      "example": false
                    },
                    "files": {
                      "type": "array",
                      "items": {
                        "type": "string",
                        "nullable": true
                      }
                    }
                  },
                  "example": {
                    "config": {
                      "type": "off"
                    },
                    "enabled": false,
                    "files": []
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/printerprofiles": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrive a mock of octoprinter printer profile",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "profiles": {
                      "$ref": "#/components/schemas/OctoProfiles"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/printer": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrive the current printer state",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "parameters": [
          {
            "in": "query",
            "name": "exclude",
            "description": "A list of attributes to not return in the response.",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "temperature": {
                      "$ref": "#/components/schemas/OctoTemperatureState"
                    },
                    "sd": {
                      "$ref": "#/components/schemas/OctoSDState"
                    },
                    "state": {
                      "$ref": "#/components/schemas/OctoPrinterState"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/printer/sd": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieves the current state of the printer’s SD card.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "$ref": "#/components/schemas/OctoSDState"
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/job": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieve information about the current job (if there is one).",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "job": {
                      "$ref": "#/components/schemas/OctoJob"
                    },
                    "progress": {
                      "$ref": "#/components/schemas/OctoProgress"
                    },
                    "state": {
                      "enum": [
                        "Operational",
                        "Printing",
                        "Pausing",
                        "Paused",
                        "Cancelling",
                        "Error",
                        "Offline"
                      ]
                    }
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "409": {
            "description": "File not found or wrong format."
          }
        }
      },
      "post": {
        "tags": [
          "octoprint"
        ],
        "summary": "Issue a job command.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "command": {
                    "type": "string",
                    "enum": [
                      "start",
                      "restart",
                      "pause",
                      "cancel"
                    ],
                    "example": "start"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "No error"
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "409": {
            "description": "If not is project, file not found or printer is printing."
          },
          "501": {
            "description": "Unsupported command."
          }
        }
      }
    },
    "/api/system/commands": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieves all configured system commands. Currently just a mock.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OctoCommands"
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/users": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieves a list of all registered users in OctoPrint. Currently just a mock.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "users": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/OctoUsers"
                      }
                    }
                  }
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/downloads/{target}/{filename}": {
      "parameters": [
        {
          "in": "path",
          "name": "target",
          "required": true,
          "description": "The target location to which to upload the file (local, sdcard).",
          "schema": {
            "type": "string",
            "default": "local",
            "enum": [
              "local",
              "sdcard"
            ]
          }
        },
        {
          "in": "path",
          "name": "filename",
          "required": true,
          "description": "The path within the location to download the file.",
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Download a file.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "description": "File not found."
          },
          "415": {
            "description": "Unsupported Media Type."
          }
        }
      }
    },
    "/api/files": {
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieve all files’ and folders’ information.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "parameters": [
          {
            "in": "header",
            "name": "If-None-Match",
            "description": "ETag",
            "schema": {
              "type": "string"
            },
            "required": false
          },
          {
            "in": "query",
            "name": "recursive",
            "schema": {
              "type": "boolean"
            },
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OctoAllFilesInfo"
                }
              }
            },
            "headers": {
              "ETag": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
    "/api/files/{target}": {
      "parameters": [
        {
          "in": "path",
          "name": "target",
          "required": true,
          "description": "The target location to which to upload the file (local, sdcard).",
          "schema": {
            "type": "string",
            "default": "local",
            "enum": [
              "local",
              "sdcard"
            ]
          }
        }
      ],
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieve all files’ and folders’ information for the target location.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "parameters": [
          {
            "in": "header",
            "name": "If-None-Match",
            "description": "ETag",
            "schema": {
              "type": "string"
            },
            "required": false
          },
          {
            "in": "query",
            "name": "recursive",
            "schema": {
              "type": "boolean"
            },
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OctoAllFilesInfo"
                }
              }
            },
            "headers": {
              "ETag": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "description": "Not Found"
          }
        }
      },
      "post": {
        "tags": [
          "octoprint"
        ],
        "summary": "Upload file or create folder.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/OctoUploadFile"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/OctoCreateFolder"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "No error",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/OctoFileUploaded"
                    },
                    {
                      "$ref": "#/components/schemas/OctoFolderCreated"
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Could not create folder or file not found."
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "409": {
            "description": "File already exist."
          },
          "415": {
            "description": "Unsupported Media Type."
          },
          "500": {
            "description": "File is too big."
          }
        }
      }
    },
    "/api/files/{target}/{filename}": {
      "parameters": [
        {
          "in": "path",
          "name": "target",
          "required": true,
          "description": "The target location to which to upload the file (local, sdcard).",
          "schema": {
            "type": "string",
            "default": "local",
            "enum": [
              "local",
              "sdcard"
            ]
          }
        },
        {
          "in": "path",
          "name": "filename",
          "required": true,
          "description": "The path within the location to the file or folder.",
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "tags": [
          "octoprint"
        ],
        "summary": "Retrieve a specific file’s or folder’s information.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "parameters": [
          {
            "in": "header",
            "name": "If-None-Match",
            "description": "ETag",
            "schema": {
              "type": "string"
            },
            "required": false
          },
          {
            "in": "query",
            "name": "recursive",
            "schema": {
              "type": "boolean"
            },
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OctoFileOrFolderInfo"
                }
              }
            },
            "headers": {
              "ETag": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "304": {
            "description": "Not Modified."
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "description": "Not Found"
          }
        }
      },
      "post": {
        "tags": [
          "octoprint"
        ],
        "summary": "Issue a file command to an existing file.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OctoFileCommand"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "No error."
          },
          "400": {
            "description": "Mandatory parameter is missing."
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "description": "Not Found."
          },
          "409": {
            "description": "If the file is currently being printed."
          },
          "415": {
            "description": "Unsupported Media Type."
          },
          "501": {
            "description": "Unsupported command."
          }
        }
      },
      "delete": {
        "tags": [
          "octoprint"
        ],
        "summary": "Delete a file or folder.",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "204": {
            "description": "No error"
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "description": "If location is neither local nor sdcard or the requested file was not found"
          },
          "409": {
            "description": "If the file to be deleted is currently being printed"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "OctoConnCurrent": {
        "type": "object",
        "properties": {
          "baudrate": {
            "type": "integer",
            "nullable": true,
            "example": null
          },
          "port": {
            "type": "string",
            "nullable": true,
            "example": null
          },
          "printerProfile": {
            "type": "string",
            "example": "_default"
          },
          "state": {
            "type": "string",
            "example": "Operational"
          }
        }
      },
      "OctoConnOptions": {
        "type": "object",
        "properties": {
          "baudratePreference": {
            "type": "string",
            "nullable": true,
            "example": null
          },
          "baudrates": {
            "type": "array",
            "items": {
              "type": "integer"
            },
            "example": []
          },
          "portPreference": {
            "type": "string",
            "nullable": true,
            "example": null
          },
          "ports": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": []
          },
          "printerProfilePreference": {
            "type": "string",
            "example": "_default"
          },
          "printerProfiles": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "example": "_default"
                },
                "name": {
                  "type": "string",
                  "example": "Prusa SL1"
                }
              }
            }
          }
        }
      },
      "OctoCommands": {
        "type": "object",
        "properties": {
          "core": {
            "type": "array",
            "default": [],
            "items": {
              "type": "object",
              "nullable": true
            }
          },
          "custom": {
            "type": "array",
            "default": [],
            "items": {
              "type": "object",
              "nullable": true
            }
          }
        }
      },
      "OctoProfiles": {
        "type": "object",
        "properties": {
          "_default": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "example": "_default"
              },
              "name": {
                "type": "string",
                "example": "Prusa SL1"
              },
              "model": {
                "type": "string",
                "example": "Original Prusa SL1"
              },
              "color": {
                "type": "string",
                "example": "default"
              },
              "current": {
                "type": "boolean",
                "example": true
              },
              "default": {
                "type": "boolean",
                "example": true
              },
              "heatedBed": {
                "type": "boolean",
                "example": false
              },
              "heatedChamber": {
                "type": "boolean",
                "example": false
              },
              "resource": {
                "type": "string",
                "example": "http://localhost/octoprint/api/printerprofiles/_default"
              }
            }
          }
        }
      },
      "timestamp": {
        "type": "number",
        "description": "Unix Timestamep"
      },
      "OctoTemperatureData": {
        "type": "object",
        "properties": {
          "actual": {
            "type": "number",
            "description": "Current temperature"
          },
          "target": {
            "type": "number",
            "description": "Target temperature",
            "nullable": true
          },
          "offset": {
            "type": "number",
            "nullable": true,
            "description": "Currenntly configured temperature offset to apply."
          }
        },
        "example": {
          "actual": 22.5,
          "target": 50,
          "offset": 1
        }
      },
      "OctoTemperatureState": {
        "type": "object",
        "properties": {
          "tool0": {
            "$ref": "#/components/schemas/OctoTemperatureData"
          },
          "tool1": {
            "$ref": "#/components/schemas/OctoTemperatureData"
          },
          "tool2": {
            "$ref": "#/components/schemas/OctoTemperatureData"
          },
          "tool3": {
            "$ref": "#/components/schemas/OctoTemperatureData"
          },
          "bed": {
            "$ref": "#/components/schemas/OctoTemperatureData"
          }
        }
      },
      "OctoSDState": {
        "type": "object",
        "properties": {
          "ready": {
            "type": "boolean",
            "example": false
          }
        }
      },
      "OctoPrinterState": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string",
            "example": "Printing"
          },
          "flags": {
            "type": "object",
            "properties": {
              "operational": {
                "type": "boolean",
                "example": false
              },
              "paused": {
                "type": "boolean",
                "example": false
              },
              "printing": {
                "type": "boolean",
                "example": true
              },
              "cancelling": {
                "type": "boolean",
                "example": false
              },
              "pausing": {
                "type": "boolean",
                "example": false
              },
              "sdReady": {
                "type": "boolean",
                "example": false
              },
              "error": {
                "type": "boolean",
                "example": false
              },
              "ready": {
                "type": "boolean",
                "example": true
              },
              "closedOrError": {
                "type": "boolean",
                "example": false
              }
            }
          }
        }
      },
      "OctoJob": {
        "type": "object",
        "properties": {
          "file": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "The name of the file without path.",
                "example": "harry_potter_hogwarts_anthony_mohimont.sl1",
                "nullable": true
              },
              "origin": {
                "enum": [
                  "local",
                  "sdcard"
                ],
                "nullable": true
              },
              "path": {
                "type": "string",
                "description": "The path to the file within the location.",
                "example": "internal/harry_potter_hogwarts_anthony_mohimont.sl1",
                "nullable": true
              },
              "date": {
                "type": "integer",
                "description": "Unix timestamp.",
                "example": 1574175255,
                "nullable": true
              },
              "size": {
                "type": "integer",
                "description": "The size of the file in bytes.",
                "example": 873151,
                "nullable": true
              }
            }
          },
          "estimatedPrintTime": {
            "type": "number",
