{
  "name": "prusa-link-web",
  "version": "2.0.0",
  "description": "This is a Prusa 3D printer webservice compatible with octoprint API.",
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx prettier . --write",
    "dev": "webpack --env dev",
    "start": "webpack serve --env dev",
    "dev:sl1": "npm run dev --env PRINTER=SL1 locales && npm run dev --env PRINTER=SL1",
    "start:sl1": "npm run start --env PRINTER=SL1 locales",
    "build:sl1": "webpack --env PRINTER=SL1 locales && webpack --env PRINTER=SL1",
    "dev:m1": "npm run dev --env PRINTER=M1 locales && npm run dev --env PRINTER=M1",
    "start:m1": "npm run start --env PRINTER=M1 locales",
    "build:m1": "webpack --env PRINTER=M1 locales && webpack --env PRINTER=M1",
    "dev:mini": "npm run dev --env PRINTER=Mini locales && npm run dev --env PRINTER=Mini",
    "start:mini": "npm run start --env PRINTER=Mini locales",
    "build:mini": "webpack --env PRINTER=Mini locales && webpack --env PRINTER=Mini"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prusa3d/Prusa-Link-Web.git"
  },
  "author": "Bruno Carvalho",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/prusa3d/Prusa-Link-Web/issues"
  },
  "homepage": "https://github.com/prusa3d/Prusa-Link-Web#readme",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "pre-commit": [
    "format"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@webpack-cli/init": "^1.0.2",
    "autoprefixer": "^10.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.0.0",
    "express-basic-auth": "^1.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0-alpha.7",
    "mini-css-extract-plugin": "^1.2.1",
    "mini-svg-data-uri": "^1.2.3",
    "multer": "^1.4.2",
    "nunjucks": "^3.2.2",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.1.0",
    "postcss-csso": "^5.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-nested": "^5.0.1",
    "pre-commit": "^1.2.2",
    "precss": "^4.0.0",
    "prettier": "2.2.1",
    "svgo": "^1.3.2",
    "terser-webpack-plugin": "^5.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.2"
  }
}
